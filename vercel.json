{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/submit-application/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/submit-quest/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "CLICKUP_API_KEY": "@clickup_api_key",
    "CLICKUP_LIST_ID": "@clickup_list_id",
    "CLICKUP_FIELD_ID_FIRST_NAME": "@clickup_field_id_first_name",
    "CLICKUP_FIELD_ID_LAST_NAME": "@clickup_field_id_last_name",
    "CLICKUP_FIELD_ID_EMAIL": "@clickup_field_id_email",
    "CLICKUP_FIELD_ID_PHONE": "@clickup_field_id_phone",
    "CLICKUP_FIELD_ID_PORTFOLIO": "@clickup_field_id_portfolio",
    "CLICKUP_FIELD_ID_RATING_REACT": "@clickup_field_id_rating_react",
    "CLICKUP_FIELD_ID_RATING_DB": "@clickup_field_id_rating_db",
    "CLICKUP_FIELD_ID_RATING_AI": "@clickup_field_id_rating_ai",
    "CLICKUP_FIELD_ID_RATING_EDGE": "@clickup_field_id_rating_edge",
    "CLICKUP_FIELD_ID_SUPERPOWER": "@clickup_field_id_superpower",
    "NOTIFICATION_WEBHOOK_URL": "@notification_webhook_url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_CLICKUP_ENDPOINT": "@next_public_clickup_endpoint"
    }
  }
} 